<PropertyList>

 <offsets>
  <!-- x/y/z == back/right/up -->
  <x-m> 0.0 </x-m>
  <y-m> 0.0 </y-m>
  <z-m> 0.0 </z-m>
 </offsets>

 <model>
  <name>Crew1</name>
  <path>Aircraft/Submarine_Scout/Models/GroundCrew/crew.xml</path>
  <offsets>
   <x-m> -1.0 </x-m>
   <y-m> -1.0 </y-m>
   <z-m>  0.0 </z-m>
  </offsets>
 </model>

 <model>
  <name>Crew2</name>
  <path>Aircraft/Submarine_Scout/Models/GroundCrew/crew.xml</path>
  <offsets>
   <x-m> -1.0 </x-m>
   <y-m>  1.0 </y-m>
   <z-m>  0.0 </z-m>
  </offsets>
 </model>

 <model>
  <name>Crew3</name>
  <path>Aircraft/Submarine_Scout/Models/GroundCrew/crew.xml</path>
  <offsets>
   <x-m>  1.0 </x-m>
   <y-m> -1.0 </y-m>
   <z-m>  0.0 </z-m>
  </offsets>
 </model>

 <model>
  <name>Crew4</name>
  <path>Aircraft/Submarine_Scout/Models/GroundCrew/crew.xml</path>
  <offsets>
   <x-m> 1.0 </x-m>
   <y-m> 1.0 </y-m>
   <z-m> 0.0 </z-m>
  </offsets>
 </model>

 <animation>
  <enable-hot type="bool">false</enable-hot>
 </animation>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5000</max-m>
 </animation>

</PropertyList>
